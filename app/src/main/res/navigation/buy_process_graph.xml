<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/buy_process_graph"
    app:startDestination="@id/cartFragment">

    <fragment
        android:id="@+id/cartFragment"
        android:name="com.ar.gptamwena.ui.shopprofile.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" >
        <action
            android:id="@+id/action_cartFragment_to_finishOrderFragment"
            app:destination="@id/finishOrderFragment" />
    </fragment>
    <fragment
        android:id="@+id/finishOrderFragment"
        android:name="com.ar.gptamwena.ui.shopprofile.FinishOrderFragment"
        android:label="fragment_finish_order"
        tools:layout="@layout/fragment_finish_order" >

        <action
            android:id="@+id/action_finishOrderFragment_to_lastFragment"
            app:destination="@id/lastFragment" />
    </fragment>
    <fragment
        android:id="@+id/lastFragment"
        android:name="com.ar.gptamwena.ui.shopprofile.LastFragment"
        android:label="fragment_last"
        tools:layout="@layout/fragment_last" />
</navigation>